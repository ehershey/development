usertool_resource_editor=copts: |saveworkspacefiles|hide|menu: Resource Editor:Resource Editorcmd: devenvotheropts: 
usertool_build_solution=copts: concur|capture|saveworkspacefiles|hide|menu: Build Soluttion:Build Solutioncmd: devenv "%w" /build "%b"otheropts: 
usertool_clean_solution=copts: concur|capture|savenone|menu: Clean Solution:Clean Solutioncmd: devenv "%w" /clean %botheropts: 
usertool_clean_project=copts: concur|capture|savenone|menu: Clean Project:Clean Projectcmd: devenv "%rp%rn.vcproj" /clean %botheropts: 
includedirs=%(INCLUDE)
sysincludedirs=
