[GLOBAL]
workingdir=.
version=7.0
packname=Microsoft Visual Studio  >= 7.0 VC++
MACRO=\n
[CONFIGURATIONS]
config=,Release
[COMPILER.Release]
compile=copts: concur|capture|savecurrent|menu: Compile:&Compilecmd: vstudiocompile "%f"otheropts: 
make=copts: concur|capture|saveworkspacefiles|menu: Build:&Buildcmd: devenv "%w" /build "%b" /project "%rm"otheropts: 
rebuild=copts: concur|capture|saveworkspacefiles|menu: Rebuild:&Rebuildcmd: devenv "%rm" /rebuild "%b"otheropts: 
debug=copts: |menu: Debug:&Debugcmd: devenv "%w"otheropts: 
