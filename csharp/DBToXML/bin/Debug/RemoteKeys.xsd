              <xs:element name="IsVolume" type="xs:short" />
              <xs:element name="IsTransport" type="xs:short" />
              <xs:element name="IsActivity" type="xs:short" />
              <xs:element name="IsSNS" type="xs:short" />
              <xs:element name="IsMode" type="xs:short" />
              <xs:element name="KeyFlags" type="xs:base64Binary" />
            </xs:sequence>
