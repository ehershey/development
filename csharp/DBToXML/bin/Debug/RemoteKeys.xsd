<?xml version="1.0" standalone="yes"?>
<xs:schema id="RemoteKeysDBToXMLDS" targetNamespace="http://tempuri.org/RemoteKeysDBToXMLDS.xsd" xmlns:mstns="http://tempuri.org/RemoteKeysDBToXMLDS.xsd" xmlns="http://tempuri.org/RemoteKeysDBToXMLDS.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" attributeFormDefault="qualified" elementFormDefault="qualified">
  <xs:element name="RemoteKeysDBToXMLDS" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
    <xs:complexType>
