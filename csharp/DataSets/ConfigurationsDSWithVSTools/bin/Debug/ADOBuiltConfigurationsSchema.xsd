<?xml version="1.0" standalone="yes"?>
<xs:schema id="Remote_x0020_Configurations" xmlns="" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xs:element name="Remote_x0020_Configurations" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element name="Configurations">
          <xs:complexType>
            <xs:attribute name="UserID" type="xs:unsignedLong" use="required" />
            <xs:attribute name="ConfigID" type="xs:unsignedShort" use="required" />
            <xs:attribute name="ConfigName" type="xs:string" use="required" />
          </xs:complexType>
        </xs:element>
        <xs:element name="Devices">
          <xs:complexType>
            <xs:attribute name="UserID" type="xs:unsignedLong" use="required" />
            <xs:attribute name="ConfigID" type="xs:unsignedShort" use="required" />
            <xs:attribute name="DeviceID" type="xs:unsignedShort" use="required" />
            <xs:attribute name="DeviceName" type="xs:string" use="required" />
          </xs:complexType>
        </xs:element>
        <xs:element name="DeviceKeys">
          <xs:complexType>
            <xs:attribute name="UserID" type="xs:unsignedLong" use="required" />
            <xs:attribute name="ConfigID" type="xs:unsignedShort" use="required" />
            <xs:attribute name="DeviceID" type="xs:unsignedShort" use="required" />
            <xs:attribute name="KeyID" type="xs:short" use="required" />
            <xs:attribute name="KeyName" type="xs:string" use="required" />
            <xs:attribute name="LearnedID" type="xs:unsignedShort" />
          </xs:complexType>
        </xs:element>
        <xs:element name="LearnedKeys">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="LearnedData" type="xs:base64Binary" minOccurs="0" msdata:Ordinal="4" />
            </xs:sequence>
            <xs:attribute name="UserID" type="xs:unsignedLong" use="required" />
            <xs:attribute name="ConfigID" type="xs:unsignedShort" use="required" />
            <xs:attribute name="DeviceID" type="xs:unsignedShort" use="required" />
            <xs:attribute name="LearnedID" type="xs:unsignedShort" use="required" />
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:complexType>
  </xs:element>
</xs:schema>